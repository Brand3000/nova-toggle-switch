{
    "name": "naif/toggle",
    "description": "A Laravel Nova toggle switch field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\Toggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\Toggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
