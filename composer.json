{
    "name": "brand3000/nova-toggle-switch",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Brand3000\\Toggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brand3000\\Toggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
